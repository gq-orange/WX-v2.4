.container{
    margin: 0;
    padding: 0;
}
#myMap{
    width:100%;
    height:100%;
    
}

.view{
    margin: 10px;
    margin-bottom: 10px;
    border-bottom: 1px solid #bfbfbf;
}

.map_text{
    width: 100%;
    font-size: 25px;
    font-weight: normal;
    font-family: arial,helvetica, verdana, sans-serif;  
}
.icon{
    padding: 1px;
    position: absolute;
    right: 8px;  
}

.nav{
    margin:0;
    padding: 0;
    position: absolute;
    bottom: 10px;
    right: 10px
}

.info{
